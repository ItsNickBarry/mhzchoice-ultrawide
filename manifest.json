{
  "manifest_version": 2,

  "name": "MHzChoice Ultrawide",
  "version": "1.0.0",

  "description": "Scale video on MHzChoice to fit ultrawide monitors",

  "developer": {
    "name": "Nick Barry"
  },

  "homepage_url": "https://github.com/itsnickbarry/mhzchoice-ultrawide",

  "content_scripts": [
    {
      "matches": ["*://embed.vhx.tv/*"],
      "all_frames": true,
      "js": ["mhzchoice_ultrawide.js"],
      "css": ["mhzchoice_ultrawide.css"]
    }
  ]
}
